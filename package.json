{
  "name": "mobi-zipper",
  "version": "0.1.4",
  "description": "Compiles a Mobi file using calibreâ€™s `ebook-convert`. Returns a promise object.",
  "main": "dist/index.js",
  "author": "Maxwell Simmer (http://maxwellsimmer.com)",
  "scripts": {
    "ava": "npm run build && ava --verbose || true",
    "lint": "./node_modules/.bin/eslint ./*.jsx || true",
    "prepublish": "npm run build",
